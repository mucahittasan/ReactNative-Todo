{
  "expo": {
    "name": "animated-todo",
    "slug": "animated-todo",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.stealim.animatedtodo",
     
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
